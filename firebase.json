{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "cleanUrls": true,
    "redirects": [],
    "headers": [{
      "source": "**/*",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=0"
      }]
    }],
    "rewrites": [{
      "source": "/api/places", "function": "places"
    }, {
      "source": "/api/search", "function": "search"
    }]
  }
}
